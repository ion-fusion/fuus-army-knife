// Simple cases
foo // ERROR
(foo) // ERROR
[foo] // ERROR
{ x: foo } // ERROR

// Trivial let/lets
(let [(foo 5)] foo)
(let [(foo 5)] bar) // ERROR
(lets [(foo 5)] foo)
(lets [(foo 5)] bar) // ERROR

// Let vs. lets
(let [(foo 5),
      (bar (+ foo 1))] // ERROR
  bar)
(lets [(foo 5),
       (bar (+ foo 1))]
  bar)

// Lambda bindings
(lambda (x y) (+ x y))
(lambda (x y) (+ x z)) // ERROR
(|x y| (+ x y))
(|x y| (+ x z)) // ERROR
(lambda args args)
(lambda args definitely_unbound) // ERROR

// THING is defined later, which is valid
THING

// Defines
(define THING 5)

THING
(+ THING THING)

// Modules with require/only_in
(module some_module "/fusion"
  (require (only_in "foo" some_import))

  (some_import)
  (something_not_imported) // ERROR
  (+ something_not_imported some_import)) // ERROR

// Ignore everything in quote
(quote definitely_unbound_but_doesnt_matter_because_quote)

// Quasiquote madness
(quasiquote (foo baz undefined))
(quasiquote (foo baz undefined (unquote error_me))) // ERROR

(define test_case_inner_undefined im_definitely_undefined) // ERROR
